{
  "name": "portal",
  "version": "1.0.0",
  "description": "Boilerplate of webpack and mithril with msx",
  "main": "webpack.config.js",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --inline --hot --colors --port 3333",
    "build": "rm -rf public/* && ./node_modules/webpack/bin/webpack.js --build-production --progress --colors --display-error-details",
    "test": "NODE_PATH=`pwd`/app ./node_modules/mocha/bin/mocha test/ --recursive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seqs/mithril-msx-webpack.git"
  },
  "keywords": [
    "mithril",
    "webpack",
    "msx",
    "jsx"
  ],
  "author": "seqs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/seqs/mithril-msx-webpack/issues"
  },
  "homepage": "https://github.com/seqs/mithril-msx-webpack",
  "devDependencies": {
    "css-loader": "^0.14.5",
    "extract-text-webpack-plugin": "^0.8.1",
    "file-loader": "^0.8.4",
    "imports-loader": "^0.6.4",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "mithril": "git://github.com/gfiuza/msx-loader.git#249fd3c5fc4590da737dcac5081bfb4356345ce",
    "msx-loader": "^0.2.0",
    "node-libs-browser": "^0.5.2",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.12.3",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.5",
    "classnames": "^2.1.3",
    "highlight.js": "^8.7.0",
    "json-loader": "^0.5.2",
    "node-polyglot": "^0.4.1",
    "swig": "^1.4.2",
    "underscore": "^1.8.3"
  }
}
